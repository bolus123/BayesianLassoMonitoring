##                burn.in = burnin)
sim <- matrix(NA, nrow = n, ncol = nsim)
for (i in 1:nsim) {
sim[, i] <- simulate(object, nsim = n, future = FALSE, xreg = xreg)
}
fi <- rowMeans(sim)
va <- mean(colMeans((sim - fi) ^ 2))
