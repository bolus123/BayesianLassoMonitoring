readrsp <- function(x, lmin = 14, alpha = 0.05) {
res <- dfphase1::rsp(x, lmin = lmin)
sigmean <- res$p[1] < 0.05
sigvar <- res$p[2] < 0.05
sig <- (sigmean + sigvar) > 0
dd <- diff(x$fit)
dd1 <- which(dd[, 1] > 0) + 1
dd2 <- which(dd[, 2] > 0) + 1
out <- list("sigmean" = sigmean, "sigvar" = sigvar, "sig" = sig,
"outliermean" = dd1, "outliervar" = dd2)
out
}
